{
    "version": 2,
    "rootDirectory": "typescript/examples/langchain-cdp-chatbot",
    "builds": [
        {
            "src": "server.ts",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "/server.ts"
        },
        {
            "src": "/api/(.*)",
            "dest": "/server.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1",
            "continue": true
        },
        {
            "src": "/(.*)",
            "dest": "/server.ts"
        }
    ]
}