{
    "version": 2,
    "buildCommand": "cd typescript/examples/langchain-cdp-chatbot && npm install && npm run build",
    "outputDirectory": "typescript/examples/langchain-cdp-chatbot/dist",
    "devCommand": "cd typescript/examples/langchain-cdp-chatbot && npm run dev",
    "installCommand": "cd typescript/examples/langchain-cdp-chatbot && npm install",
    "builds": [
        {
            "src": "typescript/examples/langchain-cdp-chatbot/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "typescript/examples/langchain-cdp-chatbot/server.ts"
        }
    ],
    "env": {
        "OPENAI_API_KEY": "@openai_api_key",
        "DEV_CDP_API_KEY_NAME": "@dev_cdp_api_key_name",
        "DEV_CDP_API_KEY_PRIVATE_KEY": "@dev_cdp_api_key_private_key",
        "NETWORK_ID": "@network_id",
        "WALLET_TYPE": "@wallet_type"
    }
}